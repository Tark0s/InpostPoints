{% extends 'base.html.twig' %}

{% block body %}
    <div class="search-container">
        <h1>Inpost points search</h1>
        {{ form_start(form) }}
        {{ form_row(form.street) }}
        {{ form_row(form.city) }}
        {{ form_row(form.postalCode) }}
        <button type="submit">Search</button>
        {{ form_end(form) }}
    </div>
{% endblock %}
